{
    "settings": {
        "defaultView": "admin/products",
        "dispatcher": "app/admin/adminDispatcher"
    },
     "views": [{
        "path"  : "admin/home",
        "class" : "HomeView",
        "module": "app/admin/view/homeView"
    }, {
        "path"  : "admin/products",
        "class" : "ProductsView",
        "module": "app/admin/view/productsView"
    }, {
        "path"  : "admin/categories",
        "class" : "CategoriesView",
        "module": "app/admin/view/categoriesView"
    }, {
        "path"  : "admin/users",
        "class" : "UsersView",
        "module": "app/admin/view/usersView"
