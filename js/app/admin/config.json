        "path"  : "admin/stores",
        "class" : "StoresView",
        "module": "app/admin/view/storesView"
    }, {
        "path"  : "admin/templates",
        "class" : "TemplatesView",
        "module": "app/admin/view/templatesView"
    }, {
        "path"  : "admin/products/images",
        "class" : "ImagesView",
        "module": "app/admin/view/products/imagesView"
